{
    "name": "zodiac1978/dns-info",
    "description": "Adds a DNS section to the debug information and some DNS checks to the Health Check feature.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "~1.0"
    },
    "authors": [
        {
            "name": "Torsten Landsiedel",
            "homepage": "https://torstenlandsiedel.de"
        }
    ],
    "support": {
        "docs": "https://github.com/Zodiac1978/dns-info/blob/main/README.md",
        "issues": "https://github.com/Zodiac1978/dns-info/issues",
        "source": "https://github.com/Zodiac1978/dns-info/"
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-sqlite3": "*",
        "szepeviktor/phpstan-wordpress": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "scripts": {
        "analyze": "@php ./vendor/bin/phpstan analyze"
    }
}
